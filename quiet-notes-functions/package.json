    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "9.11.0",
    "firebase-functions": "3.14.1",
    "firebase-functions-test": "0.3.1",
    "typescript": "4.3.4"
  },
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0"
  }
}
