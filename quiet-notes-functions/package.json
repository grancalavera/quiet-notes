{
  "name": "quiet-notes-functions",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "build": "tsc",
    "serve": "yarn build && firebase emulators:start --only functions",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "mock-users": "GOOGLE_APPLICATION_CREDENTIALS=~/quiet-notes-firebase-adminsdk.json ts-node src/scripts/mock-users.ts"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@firebase/app-types": "0.7.0",
    "@swc/core": "1.2.95",
    "@swc/helpers": "0.2.13",
    "firebase-admin": "9.11.0",
    "firebase-functions": "3.15.7",
    "firebase-functions-test": "0.3.1",
    "quiet-notes-lib": "0.0.0",
    "typescript": "4.4.2"
  },
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0"
  }
}
